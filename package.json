{
  "name": "medad",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "PORT=5000 react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jest-environment-jsdom-sixteen",
    "test:nowatch": "CI=true react-scripts test --env=jest-environment-jsdom-sixteen",
    "cy:run": "cypress run -e 'TAGS=not @ignore or not @slow-test or not @liveNotification'",
    "cy:ci": "TZ=Africa/Cairo cypress run -e 'TAGS=not @ignore'",
    "cy:start": "REACT_APP_ENV=e2e PORT=5001 react-scripts start",
    "stop:test": "./stop_server.sh 5001",
    "e2e": "./run-e2e-tests.sh --BACKEND_PATH=$BACKEND_PATH",
    "e2e:force": "./run-e2e-tests.sh --BACKEND_PATH=$BACKEND_PATH --FORCE=true",
    "cy:run:debug": "cypress run --browser chrome",
    "cy:verify": "cypress verify",
    "cy:info": "cypress info",
    "cy:version": "cypress version",
    "cy:run:parallel": "cypress run --parallel",
    "lint": "eslint . --fix",
    "lint:nosave": "eslint --ignore-path .gitignore",
    "format": "prettier --write \"**/*.+(js|jsx|json|css|md)\"",
    "format:nosave": "prettier \"**/*.+(js|jsx|json|css|md)\"",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "@babel/runtime": "^7.7.2",
    "@brainhubeu/react-permissible": "^1.9.16",
    "@date-io/date-fns": "^1.3.13",
    "@devexpress/dx-core": "^2.6.4",
    "@devexpress/dx-react-core": "^2.6.4",
    "@devexpress/dx-react-scheduler": "^2.6.4",
    "@devexpress/dx-react-scheduler-material-ui": "^2.6.4",
    "@hooks/render-count": "^0.1.0",
    "@material-ui/core": "^4.5.2",
    "@material-ui/icons": "^4.5.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@material-ui/pickers": "^3.2.10",
    "@material-ui/styles": "^4.5.2",
    "@rails/actioncable": "^6.0.3-3",
    "@tinymce/tinymce-react": "^3.6.0",
    "action-cable-react-jwt": "0.0.5",
    "axios": "^0.19.0",
    "babel-eslint": "^10.1.0",
    "bootstrap": "^4.3.1",
    "clsx": "^1.1.1",
    "date-fns": "^2.1.0",
    "dompurify": "^2.1.1",
    "google-libphonenumber": "^3.2.10",
    "history": "^4.10.1",
    "hookrouter": "^1.2.3",
    "humps": "^2.0.1",
    "i18next": "^19.8.2",
    "immer": "^7.0.9",
    "jss-preset-default": "^10.0.0",
    "jss-rtl": "^0.3.0",
    "lodash": "^4.17.20",
    "material-table": "^1.53.0",
    "material-ui-next-responsive-table": "^0.5.1",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.31",
    "mui-datatables": "^3.3.0",
    "multiselect-react-dropdown": "^1.5.7",
    "perfect-scrollbar": "^1.5.0",
    "react": "^16.12.0",
    "react-addons-shallow-compare": "^15.6.2",
    "react-bootstrap": "0.33.0",
    "react-countdown": "^2.2.1",
    "react-datepicker": "^2.16.0",
    "react-dom": "^16.12.0",
    "react-hook-form": "^5.7.2",
    "react-i18next": "^11.7.3",
    "react-icon-tint": "^2.0.0",
    "react-images-upload": "^1.2.8",
    "react-phone-input-2": "^2.13.7",
    "react-query": "^2.23.1",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^3.2.0",
    "react-select": "^3.1.0",
    "react-select-country-list": "^2.2.1",
    "react-select-material-ui": "^6.6.3",
    "react-show-more-text": "^1.4.2",
    "react-slideshow-image": "^1.3.1",
    "react-swipeable-views": "^0.13.3",
    "react-test-renderer": "^16.11.0",
    "react-toastify": "^6.0.8",
    "react-tooltip": "^4.2.7",
    "react-with-direction": "^1.3.1",
    "styled-components": "^5.2.0",
    "webfontloader": "^1.6.28",
    "webpack-dev-server": "^3.11.0"
  },
  "devDependencies": {
    "@testing-library/dom": "^7.26.3",
    "@testing-library/react": "^11.1.0",
    "@testing-library/react-hooks": "^3.4.2",
    "axios-mock-adapter": "^1.18.2",
    "cucumber": "^6.0.5",
    "cypress": "^5.4.0",
    "cypress-cucumber-preprocessor": "^2.5.0",
    "cypress-file-upload": "^4.0.7",
    "cypress-intellij-reporter": "0.0.4",
    "cypress-localstorage-commands": "^1.2.1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-cypress": "^2.7.0",
    "eslint-plugin-no-inline-styles": "^1.0.5",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.21.2",
    "eslint-plugin-react-hooks": "^2.2.0",
    "eslint-plugin-unused-imports": "^0.1.3",
    "faker": "^4.1.0",
    "husky": "^3.0.9",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "jest-localstorage-mock": "^2.4.3",
    "lint-staged": "^9.4.2",
    "mock-socket": "^9.0.3",
    "mutationobserver-shim": "^0.3.3",
    "pm2": "^4.2.0",
    "prettier": "^1.18.2",
    "prop-types": "^15.7.2",
    "wait-for-localhost": "^3.3.0"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run test:nowatch && npm run e2e:force",
      "post-commit": "npm run stop:test"
    }
  },
  "lint-staged": {
    "!(src/__test__/**/*).js": [
      "npm run lint:nosave"
    ],
    "*.+(json|css|md)": [
      "npm run format:nosave"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "plugin:prettier/recommended",
      "plugin:cypress/recommended"
    ],
    "plugins": [
      "react-hooks"
    ],
    "rules": {
      "react-hooks/rules-of-hooks": "error",
      "react-hooks/exhaustive-deps": "warn"
    }
  },
  "prettier": {
    "singleQuote": true,
    "semi": true,
    "jsxSingleQuote": true,
    "jsxBracketSameLine": true,
    "trailingComma": "all",
    "arrowParens": "always"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}

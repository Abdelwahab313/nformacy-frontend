{
  "name": "medad",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "PORT=5000 react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --passWithNoTests",
    "cy:run": "cypress run",
    "lint": "eslint . --fix",
    "format": "prettier --write \"**/*.+(js|jsx|json|css|md)\"",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "@babel/runtime": "^7.7.2",
    "@material-ui/core": "^4.5.2",
    "@material-ui/icons": "^4.5.1",
    "@material-ui/styles": "^4.5.2",
    "apollo-boost": "^0.4.4",
    "axios": "^0.19.0",
    "bootstrap": "^4.3.1",
    "date-fns": "^2.1.0",
    "graphql": "^14.5.8",
    "graphql-tag": "^2.10.1",
    "hookrouter": "^1.2.3",
    "i18next": "^19.5.0",
    "jss-preset-default": "^10.0.0",
    "jss-rtl": "^0.3.0",
    "lodash": "^4.17.15",
    "material-table": "^1.53.0",
    "material-ui-next-responsive-table": "^0.5.1",
    "moment": "^2.24.0",
    "mui-datatables": "^2.13.1",
    "react": "^16.12.0",
    "react-addons-shallow-compare": "^15.6.2",
    "react-apollo": "^3.1.3",
    "react-bootstrap": "0.33.0",
    "react-datepicker": "^2.10.0",
    "react-dom": "^16.12.0",
    "react-google-maps": "^9.4.5",
    "react-i18next": "^9.0.10",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.2.0",
    "react-slideshow-image": "^1.3.1",
    "react-swipeable-views": "^0.13.3",
    "react-test-renderer": "^16.11.0",
    "react-with-direction": "^1.3.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.3",
    "@testing-library/react": "^9.3.2",
    "@testing-library/react-hooks": "^3.2.1",
    "axios-mock-adapter": "^1.17.0",
    "cucumber": "^6.0.5",
    "cypress": "^3.8.3",
    "cypress-cucumber-preprocessor": "^2.5.0",
    "cypress-intellij-reporter": "0.0.4",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-cypress": "^2.7.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.2.0",
    "husky": "^3.0.9",
    "lint-staged": "^9.4.2",
    "mutationobserver-shim": "^0.3.3",
    "pm2": "^4.2.0",
    "prettier": "^1.18.2",
    "react-hook-form": "^3.27.0"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && CI=true npm run test && npm run cy:run"
    }
  },
  "lint-staged": {
    "*.+(js|jsx)": [
      "npm run lint",
      "npm run format",
      "git add"
    ],
    "*.+(json|css|md)": [
      "npm run format",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "plugin:prettier/recommended",
      "plugin:cypress/recommended"
    ],
    "plugins": [
      "react-hooks"
    ],
    "rules": {
      "react-hooks/rules-of-hooks": "error",
      "react-hooks/exhaustive-deps": "warn"
    }
  },
  "prettier": {
    "singleQuote": true,
    "semi": true,
    "jsxSingleQuote": true,
    "jsxBracketSameLine": true,
    "trailingComma": "all",
    "arrowParens": "always"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
